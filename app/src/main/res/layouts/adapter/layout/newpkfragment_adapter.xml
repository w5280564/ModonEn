<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/my_Lin"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:background="@drawable/newpk_relbg"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/count_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorFristYellow"
            android:textSize="20sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/project_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorFristBlack"
            android:textSize="11sp"
            android:textStyle="bold"/>

        <ImageView
            android:id="@+id/line_Img"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:background="@color/colorSecondBlack"
            android:alpha="0.5"/>

        <TextView
            android:id="@+id/all_Txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorSecondBlack"
            android:textSize="10sp"
            android:textStyle="bold" />

    </LinearLayout>

    <TextView
        android:id="@+id/rank_Txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#f55e6b"
        android:textSize="10sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/colock_Img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/my_Lin"
        android:layout_marginLeft="5dp"
        android:src="@drawable/daypk_colock_icon"
        android:visibility="gone" />


    <ImageView
        android:id="@+id/istrain_Img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:src="@drawable/istrain_icon"
        android:visibility="gone"/>


