<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    android:orientation="vertical">


    <TextView
        android:id="@+id/report_one_Txt"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorFristWhite"
        android:gravity="center"
        android:text="数据虚假"
        android:textColor="@color/colorheadBlue"
        android:textSize="16sp"
        android:visibility="gone" />

    <TextView
        android:id="@+id/report_two_Txt"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorFristWhite"
        android:gravity="center"
        android:text="广告"
        android:textColor="@color/colorheadBlue"
        android:textSize="16sp"
        android:visibility="gone" />

    <TextView
        android:id="@+id/report_three_Txt"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorFristWhite"
        android:gravity="center"
        android:text="不友善行为"
        android:textColor="@color/colorheadBlue"
        android:textSize="16sp"
        android:visibility="gone" />

    <TextView
        android:id="@+id/report_four_Txt"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorFristWhite"
        android:gravity="center"
        android:text="其他"
        android:textColor="@color/colorheadBlue"
        android:textSize="16sp"
        android:visibility="gone" />

    <TextView
        android:id="@+id/report_five_Txt"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorFristWhite"
        android:gravity="center"
        android:text="删除"
        android:textColor="@color/colorheadBlue"
        android:textSize="16sp"
        android:visibility="gone" />


</LinearLayout>