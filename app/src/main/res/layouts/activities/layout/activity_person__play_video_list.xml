<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_person__play_video_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAllBg">

    <include
        android:id="@+id/title_Include"
        layout="@layout/base_titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:id="@+id/stopTime_Lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/title_Include"
        android:orientation="vertical"
        android:background="@color/colorFristWhite"/>


</RelativeLayout>
